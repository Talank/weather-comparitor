{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --host",
    "prettify": "npx prettier --write --plugin-search-dir=. --svelte-sort-order scripts-markup-styles \"src/**\" \"tests/**\"",
    "lint": "npx prettier --check --svelte-sort-order scripts-markup-styles \"src/**\" \"tests/**\" && npx eslint src/components/*.svelte",
    "test": "jest tests",
    "test-e2e": "cypress run"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "babel-jest": "^25.5.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "jest": "^25.5.4",
    "jest-each": "^26.4.2",
    "prettier": "^2.1.2",
    "prettier-plugin-svelte": "^1.4.0",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.29.0",
    "svelte-jester": "^1.1.5"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.2.1",
    "cypress": "^5.3.0",
    "sirv-cli": "^0.4.6"
  }
}
